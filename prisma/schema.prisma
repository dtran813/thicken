// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Menu {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  price Float
  calories String
  description String
} 